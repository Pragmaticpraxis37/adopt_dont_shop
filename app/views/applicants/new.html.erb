<%= form_tag '/applicants' do %>
  <h2>Create an Application</h2>
  <%= label_tag :name %>
  <%= text_field_tag :name %><br>
  <%= label_tag :address %>
  <%= text_field_tag :address %><br>
  <%= label_tag :city %>
  <%= text_field_tag :city, required: true%><br>
  <%= label_tag :state %>
  <%= text_field_tag :state %><br>
  <%= label_tag :zip %>
  <%= text_field_tag :zip%><br>
  <%= hidden_field_tag :application_status, "In Progress" %><br>
  <%= submit_tag 'Create Application' %>
<% end %>
